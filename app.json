{
  "expo": {
    "extra": {
      "mapboxAccessToken": "sk.eyJ1Ijoic3BhcnRhbjMiLCJhIjoiY202azUwYzdtMDlrMzJqcXlqNXlyZHJ4MSJ9.94w1WJDzhFALhYjXd9rTCg",
      "router": {
